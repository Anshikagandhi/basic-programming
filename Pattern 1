

#include<iostream>
using namespace std;


int main(){

      int n,i=1;
      cin>>n;

   	for(i=1;i<=n;i++)
    {

      int count,val=i;
        for(count=1;count<=n-i;count=count+1)
        {
            cout<<" ";
        }



        for(count=1;count<=i;count=count+1)
    	{cout<<val;
      	val=val+1;

          }
  cout<<endl;
}
}
