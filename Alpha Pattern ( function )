#include<iostream>
using namespace std;


int main(){
  int n,j,i;
  cin>>n;
  char value='A';
  
  for(i=1;i<=n;i++)
  {
    for(j=1;j<=i;j++)
    {
      cout<<value;
      
      
      
    }
   value++;
  
  cout<<endl;}
   
  }


    
    /*  Read input as specified in the question.
     * Print output as specified in the question.
     */
    


