/*
  Print following pattern for N
  Here, N = 4
	1234
	123
	12
	1
*/
#include<iostream>
using namespace std;

int main() {
	// Write your code here
int n,i=1;
cin>>n;

while(i<=n)
{

    int count=1;
    int value=1;
    while(count<=(n+1)-i)
    {
        cout<<value;
        value=value+1;
        count=count+1;
    }

i=i+1;
cout<<endl;
}

    }
