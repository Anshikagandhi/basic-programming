#include<iostream>
using namespace std;

int main() {
	// Write your code here
int n,i=2;
cin>>n;
if(n>=1)
{
    cout<<"1"<<endl;
}
while(i<=n)
{

    int count=1;
    while(count<=i)
    {
        if(count==i || count==1)
        {
            cout<<i-1;
        }
        else{cout<<0;}

count=count+1;

}
i=i+1;
cout<<endl;
    }
}
