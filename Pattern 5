/*
Print pattern for N,
Here N=5

	1
	11
	121
	1221
	12221
*/

#include<iostream>
using namespace std;

int main() {
int n,i=2;
cin>>n;
if(n>=1)
{
    cout<<"1"<<endl;
}
while(i<=n)
{

    int count=1;
    while(count<=i)
    {
        if(count==i || count==1)
        {
            cout<<i-1;
        }
        else{cout<<0;}

count=count+1;

}
i=i+1;
cout<<endl;
    }
}
